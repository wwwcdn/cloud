/* CSS 排行榜 */
a{color:#000; text-decoration:none;}
a:hover{color:#f60; text-decoration:underline;}
.top8{margin-top:8px;}
.clear{clear:both; height:0px; overflow:hidden; line-height:0px; font-size:0px;}
.phb_contain{width:960px; margin:8px auto 0 auto; background-color:#FFFFFF;OVERFLOW: hidden}
.phb_left{ float:left; width:160px; background-color:#f6f8f7; padding-bottom:20px}
.phb_right{ float:right; width:776px;background-color:#f6f8f7; padding:5px;OVERFLOW: hidden; }
.phb_kuang{ border:1px solid #d9d7d7}
.phb_kuang2{ border:1px solid #fed5b5;}
.phb_bt02{ height:24px; background-image: url(../img/ph02.jpg); background-repeat: no-repeat; text-align:center; padding-top:5px; font-size:14px; color:#FFFFFF; background-position:center; font-weight: bold; margin-bottom:10px}
.phb_left #box { PADDING: 5px 8px;}
.phb_left #box LI { TEXT-ALIGN: center; LINE-HEIGHT: 32px; FONT-SIZE: 14px }
.phb_left #box LI A { COLOR: #000;FONT-SIZE: 14px }
.phb_left #box LI A:visited { COLOR: #000;FONT-SIZE: 14px }
.phb_left #box LI A:hover { COLOR: #FF6600;FONT-SIZE: 14px}
.phb_left #box .a1{ background-image: url(../img/ph03.jpg); background-repeat: no-repeat; width:144px; height:27px; font-weight: bold; color:#FF6600}
.phb_left #box .a1 *{color:#FF6600}
.phb_left .line { BACKGROUND-IMAGE: url(../img/listbg.gif); height:1px; background-repeat: no-repeat; background-position: center; }
.phb_bt01{ background-image: url(../img/ph01.jpg); background-repeat: no-repeat; background-position: left top;height:35px; border:1px solid #d9d7d7;text-align:right;}
.phb_bt01 a{ float:right; margin-right:10px; margin-top:8px}
.phb_tr3 { WIDTH: 802px; OVERFLOW: hidden; }
.phb_tr3 .mod{ margin-top:10px; width:249px;height:385px; margin-right:11px;FLOAT:left;}
.phb_tr3 .mod_l{ float:left; width:116px;FONT-SIZE: 14px ;padding:8px 0px 0 10px; text-align:left}
.phb_tr3 .mod_r{ float:right; width:115px;height:33px;}
/**/
.phb_tr3 .mod_r ul{height:26px;}
.phb_tr3 .mod_r li{display: inline;}
.phb_tr3 .mod_r li a{ width:32px;float:left; height:33px; text-align:center; line-height:33px; color:#666666; background-position: right center;background-image: url(../img/phb_jg.jpg); background-repeat: no-repeat;}
.phb_tr3 .mod_r .hover a{width:41px;background-image: url(../img/ph05.jpg);background-repeat: no-repeat; color:#FF6600 }

.phb_main{ border-left:1px solid #d9d7d7;border-right:1px solid #d9d7d7;background-image: url(../img/phb_pm.jpg);background-repeat: no-repeat; background-position: 10px 10px;OVERFLOW: hidden;padding-top:10px; width:247px;background-color:#FFFFFF; }
.phb_main UL LI {LINE-HEIGHT: 21px; WIDTH:210px; FLOAT: left; HEIGHT: 21px; OVERFLOW: hidden; PADDING:6px 0 2px 38px; text-align:left}
.phb_main UL LI SPAN {display: block;OVERFLOW: hidden }
.phb_main UL LI CITE {display: block;OVERFLOW: hidden }
.phb_main UL LI SPAN {WIDTH: 130px; FLOAT: left;HEIGHT: 22px }
.phb_main UL LI CITE {TEXT-ALIGN: right; WIDTH: 70px; FLOAT: right; HEIGHT: 22px; font-style: normal; padding-right:10px }
.phb_main UL LI CITE A {COLOR: #888;}
.phb_main UL LI CITE A:hover { COLOR: #f00}
.phb_main UL .a3{border-bottom:1px dashed #e8e4e4}
.phb_main UL .a1{ background-color:#f6f7f7; border-top:1px solid #e8e4e4; line-height:26px;text-align:right; height:26px; border-bottom:1px solid #d9d7d7}
.phb_main UL .a2{background-color:#fef4eb; border-top:1px solid #fed5b5; line-height:26px;text-align:right; height:26px ; border-bottom:1px solid #fed5b5}
/**/
.phb_bt03{ height:34px; background-image: url(../img/ph_d1.jpg); background-repeat: repeat-x; border:1px solid #d9d7d7; border-bottom:0  }
.phb_bt04{ height:26px; background-image: url(../img/ph_d2.jpg); background-repeat: repeat-x; padding:8px 0px 0 10px; color:#d51515;FONT-SIZE: 14px ; border:1px solid #fed5b5; border-bottom:0 }
.phb_bt05{ height:26px; background-image: url(../img/ph_d1.jpg); background-repeat: repeat-x; padding:8px 0px 0 10px;FONT-SIZE: 14px ;text-align:left;}
/*列表页*/
.phb_right2{ float:right; width:786px; }
.phb_lby{ PADDING-BOTTOM: 12px; PADDING-LEFT: 20px; WIDTH: 233px; PADDING-RIGHT: 5px; PADDING-TOP:10px ; float:left}
.mod{ width:784px;}
.phb_lby .no1 { POSITION: relative; PADDING-LEFT: 2px; WIDTH: 233px; MARGIN-BOTTOM: 10px; HEIGHT: 110px;TEXT-ALIGN:left }
.phb_lby .no1 .TOP1{POSITION:absolute; MARGIN: 8px 0px 0px -2px; WIDTH: 51px; display: block; HEIGHT: 51px; LEFT: 0px; background-image: url(../img/top_01.gif); background-repeat: no-repeat; background-position: 0px 0px; }
.phb_lby .no1 .TOP2{ POSITION:absolute; MARGIN: 8px 0px 0px -2px;WIDTH: 51px; display: block; HEIGHT: 51px; LEFT: 0px; background-image: url(../img/top_02.gif); background-repeat: no-repeat; background-position: 0px 0px; }
.phb_lby .no1 .TOP3{ POSITION:absolute; MARGIN: 8px 0px 0px -2px; WIDTH: 51px; display: block; HEIGHT: 51px; LEFT: 0px; background-image: url(../img/top_03.gif); background-repeat: no-repeat; background-position: 0px 0px;}
.phb_lby .no1 IMG { BORDER: #ddd 1px solid;PADDING: 3px;WIDTH: 80px;display: block; HEIGHT: 102px; margin-right:8px }
.phb_lby .no1 A:hover IMG { BORDER: #f5bc58 1px solid;BACKGROUND: #f5bc58;  }
.phb_lby .no1 .toptext { WIDTH: 136px; PADDING-TOP: 3px; POSITION:absolute; z-index:2; margin-left:100PX; left: 0px; top: 0px; }
.phb_lby .no1 .toptext STRONG { WIDTH: 136px; display: block;  FLOAT: left; font-family: "微软雅黑"; font-size: 16px; }
.phb_lby .no1 .toptext P {WIDTH: 136px; display: block; FLOAT: left; OVERFLOW: hidden; }
.phb_lby .no1 .toptext STRONG { 	MARGIN-BOTTOM: 2px; HEIGHT: 23px; }
.phb_lby .no1 .toptext STRONG A { COLOR:#FF6600;}
.phb_lby .no1 .toptext P { LINE-HEIGHT: 20px; TEXT-INDENT: 2em; COLOR: #888 }
.phb_lby .no1 .toptext P A { LINE-HEIGHT: 20px; COLOR: #ec6200 }
.phb_lby .no1 .toptext P A:hover { COLOR: #f00 }

